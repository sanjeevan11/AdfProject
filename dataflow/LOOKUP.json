{
	"name": "LOOKUP",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "emplSource",
						"type": "DatasetReference"
					},
					"name": "EMPL"
				},
				{
					"dataset": {
						"referenceName": "DeptSource",
						"type": "DatasetReference"
					},
					"name": "DEPT"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmpNo as integer,",
				"          EmpName as string,",
				"          Sal as integer,",
				"          Deptno as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EMPL",
				"source(output(",
				"          Deptno as integer,",
				"          Dname as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DEPT",
				"EMPL, DEPT join(EMPL@Deptno == DEPT@Deptno,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          EmployeeNO = EmpNo,",
				"          EmpName,",
				"          Sal,",
				"          Deptno = EMPL@Deptno,",
				"          Deptno = DEPT@Deptno,",
				"          Deptname = Dname",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}