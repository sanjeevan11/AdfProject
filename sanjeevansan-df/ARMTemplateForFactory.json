					],
					"scriptLines": [
						"source(output(",
						"          EmpNo as integer,",
						"          EmpName as string,",
						"          Sal as integer,",
						"          Deptno as integer",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> EMPL",
						"source(output(",
						"          Deptno as integer,",
						"          Dname as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> DEPT",
						"EMPL, DEPT join(EMPL@Deptno == DEPT@Deptno,",
						"     joinType:'inner',",
						"     matchType:'exact',",
						"     ignoreSpaces: false,",
						"     broadcast: 'auto')~> join1",
						"join1 window(desc(EMPL@Deptno, true),",
						"     Sal = avg(Sal)) ~> window1",
						"window1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     partitionFileNames:['window.txt'],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     partitionBy('hash', 1)) ~> sink1"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/emplSource')]",
				"[concat(variables('factoryId'), '/datasets/DeptSource')]",
				"[concat(variables('factoryId'), '/datasets/windowOutput')]"
			]
		}
	]
